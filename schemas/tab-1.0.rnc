default namespace tab = "http://schemas.eldergods.com/tab/v1"
#namespace xs = "http://www.w3.org/2001/XMLSchema-datatypes"
#datatypes xs = "http://www.w3.org/2001/XMLSchema-datatypes"

grammar {
	start = TAB
	TAB =
		element tab { TAB_TYPE }
	TAB_TYPE = 
		attribute id { text } &
		attribute windowId { text }? &
		attribute index { text }? &
		element url { text }? &
		element title { text }? &
		element favIconUrl { text }? &
		element opener { text }? &
		element referrer { text }? &
		element event { "new" | "change" | "close" }?
}
